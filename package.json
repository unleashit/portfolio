{
  "name": "next.jasongallagher.org",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "NODE_OPTIONS='--inspect' next dev | pino-pretty",
    "build": "next build",
    "start": "NODE_ENV=production http-server -p 3000 out",
    "test": "vitest",
    "lint": "next lint",
    "format": "prettier '**/*.{js,jsx,ts,tsx}' --write",
    "ts": "tsc --noEmit --incremental --watch",
    "create-snapshot": "docker exec directus npx directus schema snapshot ./database/snapshots/\"$(date \"+%F\")\"-snapshot-\"$(date \"+%s\")\".yaml"
  },
  "dependencies": {
    "next-promise-cache": "^0.5.3",
    "bootstrap": "^5.2.3",
    "lodash": "^4.17.21",
    "next": "^13.2.5-canary.25",
    "pino": "^8.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "server-only": "^0.0.1"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@next/bundle-analyzer": "^13.2.4",
    "@testing-library/react": "^14.0.0",
    "@types/lodash": "^4.14.192",
    "@types/node": "18.15.11",
    "@types/react": "18.0.33",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "c8": "^7.13.0",
    "eslint": "8.37.0",
    "eslint-config-next": "^13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "jsdom": "^21.1.1",
    "pino-pretty": "^10.0.0",
    "postcss": "^8.4.21",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^8.3.0",
    "prettier": "^2.8.7",
    "sass": "^1.60.0",
    "typescript": "^5.0.3",
    "vitest": "^0.29.8"
  },
  "prettier": {
    "singleQuote": true
  }
}
